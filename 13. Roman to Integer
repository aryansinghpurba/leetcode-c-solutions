#include<string.h>
int value(char ch){
    switch(ch){
        case 'M': return 1000;
        case 'D': return 500;
        case 'C': return 100;
        case 'L': return 50;
        case 'X': return 10;
        case 'V': return 5;
        case 'I': return 1;
        default: return 0;
    }
}
int romanToInt(char* s) {
    int temp=0;
    int n= strlen(s);
    for(int i=0;i<n;i++){
        int curr=value(s[i]);
        int next= value(s[i+1]);
        if(curr>=next){
            temp+=curr;
        }else{
            temp+=(next-curr);
            ++i;
        }
    }
    return temp;
}
